@using DatabaseInteraction.Models
@using System.Linq
@model UserLogin

@{ 
    Layout = null;
}


<ul class="w3-ul w3-card-4 w3-white">

    @{


        foreach (User s in Model.OtherTeachers)
        {

            <li class="w3-padding-16">

                <img src="~/Images/Dashboard/avatar2.png" class="w3-left w3-circle w3-margin-right" style="width:35px">
                <span class="w3-xlarge">Name: @s.FirstName  @s.LastName, Class: @s.Class, Phone: @s.PhoneNumber</span><br>

            </li>

        }
    }

    </ul>







